<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.adept.ims.authorization.database.pojo.PersonRole" table="PERSONROLE">
    <id column="PERSON_ID" name="id">
      <generator class="increment"/>
    </id>
    <property column="ROLE_ID" name="roleId" not-null="true"/>
    <!-- This needs to be ID in actual implementation -->
  </class>
</hibernate-mapping>
